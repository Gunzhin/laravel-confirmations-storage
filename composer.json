{
    "name": "gunzhin/laravel-confirmations-storage",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "gunzhin",
        "laravel-confirmations-storage"
    ],
    "homepage": "https://github.com/gunzhin/laravel-confirmations-storage",
    "license": "MIT",
    "authors": [
        {
            "name": "Eliahu Gunzhin",
            "email": ":author_email",
            "homepage": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "gunzhin\\laravel-confirmations-storage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gunzhin\\laravel-confirmations-storage\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
